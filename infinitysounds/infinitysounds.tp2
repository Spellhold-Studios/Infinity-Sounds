// patch generic wizard on-cast sounds
DEFINE_PATCH_FUNCTION pch_wiz_cas_sfx STR_VAR wiz_cas_sfx = ~~ BEGIN
  READ_ASCII 0x10 resref
  PATCH_IF (~%resref%~ STRING_COMPARE_REGEXP ~cas_[mp]0[1-8]\|eff_m11a~ == 0) BEGIN
    WRITE_ASCIIE 0x10 ~%wiz_cas_sfx%~ #8
  END
END

// patch generic wizard post-cast sounds
DEFINE_PATCH_FUNCTION pch_wiz_eff_sfx STR_VAR wiz_eff_sfx = ~~ BEGIN
  READ_LONG  0x64 abl_off
  READ_SHORT 0x68 abl_cnt
  READ_LONG  0x6a gfx_off
  READ_SHORT 0x6e gfx_idx
  READ_SHORT 0x70 gfx_cnt
  FOR (i = 0; i < gfx_cnt; i += 1) BEGIN
    READ_SHORT (gfx_off + 0x30*(gfx_idx + i)) opcode
    READ_ASCII (gfx_off + 0x14 + 0x30*(gfx_idx + i)) resref
    PATCH_IF (opcode == 174) && (~%resref%~ STRING_COMPARE_REGEXP ~eff_[mp]0[1-8]\|eff_m33~ == 0) BEGIN
      WRITE_ASCIIE (gfx_off + 0x14 + 0x30*(gfx_idx + i)) ~%wiz_eff_sfx%~ #8
    END
  END
  FOR (i = 0; i < abl_cnt; i += 1) BEGIN
    READ_SHORT (abl_off + 0x28*i + 0x1e) fx_cnt
    READ_SHORT (abl_off + 0x28*i + 0x20) fx_idx
    FOR (j = 0; j < fx_cnt; j += 1) BEGIN
      READ_SHORT (gfx_off + 0x30*(fx_idx + j)) opcode
      READ_ASCII (gfx_off + 0x14 + 0x30*(fx_idx + j)) resref
      PATCH_IF (opcode == 174) && (~%resref%~ STRING_COMPARE_REGEXP ~eff_[mp]0[1-8]\|eff_m33~ == 0) BEGIN
        WRITE_ASCIIE (gfx_off + 0x14 + 0x30*(fx_idx + j)) ~%wiz_eff_sfx%~ #8
      END
    END
  END
END

