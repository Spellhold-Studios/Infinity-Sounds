COPY_EXISTING_REGEXP GLOB ~^.+\.spl$~ ~override~
  READ_SHORT 0x1c type
  READ_SHORT 0x22 anim
  READ_BYTE  0x25 school
  PATCH_IF (type == 1) && (VARIABLE_IS_SET $ary_wiz_cas_sfx(~%anim%~)) BEGIN
    TEXT_SPRINT wiz_cas_sfx $ary_wiz_cas_sfx(~%anim%~)
    LPF pch_wiz_cas_sfx STR_VAR wiz_cas_sfx END
  END
  ELSE BEGIN
    PATCH_IF (type == 1) && (VARIABLE_IS_SET $ary_fbk_wiz_cas_sfx(~%school%~)) BEGIN
      TEXT_SPRINT wiz_cas_sfx $ary_fbk_wiz_cas_sfx(~%school%~)
      LPF pch_wiz_cas_sfx STR_VAR wiz_cas_sfx END
    END
  END
  PATCH_IF (type == 1) && (VARIABLE_IS_SET $ary_wiz_eff_sfx(~%anim%~)) BEGIN
    TEXT_SPRINT wiz_eff_sfx $ary_wiz_eff_sfx(~%anim%~)
    LPF pch_wiz_eff_sfx STR_VAR wiz_eff_sfx END
  END
  ELSE BEGIN
    PATCH_IF (type == 1) && (VARIABLE_IS_SET $ary_fbk_wiz_eff_sfx(~%school%~)) BEGIN
      TEXT_SPRINT wiz_eff_sfx $ary_fbk_wiz_eff_sfx(~%school%~)
      LPF pch_wiz_eff_sfx STR_VAR wiz_eff_sfx END
    END
  END
  PATCH_IF (type == 2) && (VARIABLE_IS_SET $ary_prt_cas_sfx(~%anim%~)) BEGIN
    TEXT_SPRINT prt_cas_sfx $ary_prt_cas_sfx(~%anim%~)
    LPF pch_prt_cas_sfx STR_VAR prt_cas_sfx END
  END
  ELSE BEGIN
    PATCH_IF (type == 2) && (VARIABLE_IS_SET $ary_fbk_prt_cas_sfx(~%school%~)) BEGIN
      TEXT_SPRINT prt_cas_sfx $ary_fbk_prt_cas_sfx(~%school%~)
      LPF pch_prt_cas_sfx STR_VAR prt_cas_sfx END
    END
  END
  PATCH_IF (type == 2) && (VARIABLE_IS_SET $ary_prt_eff_sfx(~%anim%~)) BEGIN
    TEXT_SPRINT prt_eff_sfx $ary_prt_eff_sfx(~%anim%~)
    LPF pch_prt_eff_sfx STR_VAR prt_eff_sfx END
  END
  ELSE BEGIN
    PATCH_IF (type == 2) && (VARIABLE_IS_SET $ary_fbk_prt_eff_sfx(~%school%~)) BEGIN
      TEXT_SPRINT prt_eff_sfx $ary_fbk_prt_eff_sfx(~%school%~)
      LPF pch_prt_eff_sfx STR_VAR prt_eff_sfx END
    END
  END
  PATCH_IF (VARIABLE_IS_SET $ary_exp_sfx(~%anim%~)) BEGIN
    TEXT_SPRINT exp_sfx $ary_exp_sfx(~%anim%~)
    LPF pch_exp_sfx STR_VAR exp_sfx END
  END
  ELSE BEGIN
    PATCH_IF (VARIABLE_IS_SET $ary_fbk_exp_sfx(~%school%~)) BEGIN
      TEXT_SPRINT exp_sfx $ary_fbk_exp_sfx(~%school%~)
      LPF pch_exp_sfx STR_VAR exp_sfx END
    END
  END
BUT_ONLY IF_EXISTS

