// horse whinny
COPY_EXISTING ~%Nashkel%.are~          ~override~
              ~%Beregost%.are~         ~override~
              ~%NWBaldursGate%.are~    ~override~
              ~%NBaldursGate%.are~     ~override~
              ~%NEBaldursGate%.are~    ~override~
              ~%Farmlands%.are~        ~override~
              ~%WBaldursGate%.are~     ~override~
              ~%EBaldursGate%.are~     ~override~
              ~%SWBaldursGate%.are~    ~override~
              ~%BaldursGateDocks%.are~ ~override~
              ~%FishingVillage%.are~   ~override~
  READ_SHORT 0x82 amb_cnt
  READ_LONG  0x84 amb_off
  FOR (i = 0; i < amb_cnt; i += 1) BEGIN
    READ_SHORT (amb_off + 0xd4*i + 0x80) snd_cnt
    FOR (j = 0; j < snd_cnt; j += 1) BEGIN
      READ_ASCII (amb_off + 0xd4*i + 0x30 + 0x08*j) resref
      PATCH_IF (~%resref%~ STRING_COMPARE_REGEXP ~amb_e[34][56][abc]~ == 0) BEGIN
        WRITE_LONG  (amb_off + 0xd4*i + 0x28)  4 // pitch variation
        WRITE_SHORT (amb_off + 0xd4*i + 0x2c) 10 // volume variation
        WRITE_SHORT (amb_off + 0xd4*i + 0x2e) 70 // volume
        WRITE_LONG  (amb_off + 0xd4*i + 0x84) 20 // interval
        WRITE_LONG  (amb_off + 0xd4*i + 0x88) 10 // interval variation
        WRITE_LONG  (amb_off + 0xd4*i + 0x90)  9 // random order
        WRITE_LONG  (amb_off + 0xd4*i + 0x8c) 131040 // 5:30-17:30
        WRITE_ASCII (amb_off + 0xd4*i + 0x30) ~amb_e35c~ #8 // sound 1
        WRITE_ASCII (amb_off + 0xd4*i + 0x38) ~amb_e46a~ #8 // sound 2
        WRITE_ASCII (amb_off + 0xd4*i + 0x40) ~amb_e46b~ #8 // sound 3
        WRITE_SHORT (amb_off + 0xd4*i + 0x80)  3 // sound count
        FOR (k = 0; k < 7; k += 1) BEGIN
          WRITE_ASCII (amb_off + 0xd4*i + 0x48 + 0x08*k) ~~ #8
        END
      END
    END
  END
BUT_ONLY IF_EXISTS

