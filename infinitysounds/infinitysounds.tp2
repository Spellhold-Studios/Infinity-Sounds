BEGIN @250 DESIGNATED 250 LABEL ~s@-IS-Tweak-Existence-Sound-Delay~
REQUIRE_PREDICATE (GAME_IS ~tob bgee bg2ee eet~) @1
REQUIRE_COMPONENT ~setup-infinitysounds.tp2~ ~230~ @9

PRINT @11 SILENT

// existence delay
