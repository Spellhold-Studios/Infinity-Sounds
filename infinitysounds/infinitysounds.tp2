  PRETTY_PRINT_2DA
BUT_ONLY

// override existence sound delay
COPY_EXISTING_REGEXP GLOB ~^.+\.cre$~ ~override~
  TO_LOWER SOURCE_RES
  READ_ASCII 0x280 deathvar (32) NULL
  PATCH_IF (FILE_CONTAINS_EVALUATED (~infinitysounds/lib/250_cre_idle.tph~ ~%SOURCE_RES%\.cre~) ||
            VARIABLE_IS_SET $ary_idle_delay_incl(~%SOURCE_RES%~)) &&
           (!VARIABLE_IS_SET $ary_idle_delay_excl(~%SOURCE_RES%~)) &&
           (!FILE_CONTAINS_EVALUATED (~pdialog.2da~ ~^%deathvar%~)) BEGIN
    READ_SLONG EXISTANCE1 idle1
    PATCH_IF (idle1 > 0) BEGIN
      READ_LONG 0x28 anim
      LPF DELETE_CRE_EFFECT INT_VAR opcode_to_delete = 294 END
      PATCH_IF (VARIABLE_IS_SET $ary_idle_delay_cre(~%SOURCE_RES%~)) BEGIN
        SET param1 = $ary_idle_delay_cre(~%SOURCE_RES%~)
        LPF ADD_CRE_EFFECT INT_VAR opcode = 294 target = 1 parameter1 = %param1% timing = 9 END
      END
      ELSE BEGIN
        PATCH_IF (VARIABLE_IS_SET $ary_idle_delay_anim(~%anim%~)) BEGIN
          SET param1 = $ary_idle_delay_anim(~%anim%~)
          LPF ADD_CRE_EFFECT INT_VAR opcode = 294 target = 1 parameter1 = %param1% timing = 9 END
        END
      END
      PATCH_IF (!VARIABLE_IS_SET $ary_idle_delay_cre(~%SOURCE_RES%~)) &&
               (!VARIABLE_IS_SET $ary_idle_delay_anim(~%anim%~)) BEGIN
        LPF ADD_CRE_EFFECT INT_VAR opcode = 294 target = 1 parameter1 = %def_idle_delay% timing = 9 END
      END
    END
  END
BUT_ONLY IF_EXISTS

