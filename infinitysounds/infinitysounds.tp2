// patch creatures to remove cre attack sounds
COPY_EXISTING_REGEXP GLOB ~^.+\.cre$~ ~override~
  TO_LOWER SOURCE_RES
  READ_LONG 0x28 anim
  PATCH_IF (VARIABLE_IS_SET $ary_rmv_att_snd_cre_anim(~%anim%~)) &&
           (!VARIABLE_IS_SET $ary_rmv_att_snd_excl_cre(~%SOURCE_RES%~)) BEGIN
    READ_SLONG ATTACK1 attack1
    PATCH_IF (attack1 > 0) BEGIN
      WRITE_LONG ATTACK1 "-1"
      WRITE_LONG ATTACK2 "-1"
      WRITE_LONG ATTACK3 "-1"
      WRITE_LONG ATTACK4 "-1"
    END
  END
