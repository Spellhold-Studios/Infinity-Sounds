// TobEx: reactivate creature existence sounds
// original            TobEx/WeiDU
// PICKED_POCKET     = PICKED_POCKET
// HIDDEN_IN_SHADOWS = EXISTANCE1
// SPELL_DISRUPTED   = EXISTANCE2
// SET_A_TRAP        = EXISTANCE3
// EXISTANCE4        = EXISTANCE4
// EXISTANCE5        = EXISTANCE5
ACTION_IF (GAME_IS ~tob~) BEGIN
  // clean up pdialog for joinable NPC check
  COPY_EXISTING ~pdialog.2da~ ~override~
    PRETTY_PRINT_2DA
  BUT_ONLY

  // (if not joinable NPC) move existence sounds to TobEx-corrected slots
  COPY_EXISTING_REGEXP GLOB ~^.+\.cre$~ ~override~
    READ_ASCII 0x280 deathvar (32) NULL
    PATCH_IF (!FILE_CONTAINS_EVALUATED (~pdialog.2da~ ~^%deathvar%~)) BEGIN
      READ_SLONG PICKED_POCKET     idle1
      READ_SLONG HIDDEN_IN_SHADOWS idle2
      READ_SLONG SPELL_DISRUPTED   idle3
      READ_SLONG SET_A_TRAP        idle4
      READ_SLONG EXISTANCE4        idle5
      PATCH_IF (idle1 > 0) BEGIN
        WRITE_LONG PICKED_POCKET "-1"
        WRITE_LONG EXISTANCE1 idle1
      END
      PATCH_IF (idle1 > 0 && idle2 > 0) BEGIN
        WRITE_LONG EXISTANCE1 idle1
        WRITE_LONG EXISTANCE2 idle2
      END
      PATCH_IF (idle1 > 0 && idle3 > 0) BEGIN
        WRITE_LONG EXISTANCE2 idle2
        WRITE_LONG EXISTANCE3 idle3
      END
      PATCH_IF (idle1 > 0 && idle4 > 0) BEGIN
        WRITE_LONG EXISTANCE3 idle3
        WRITE_LONG EXISTANCE4 idle4
      END
      PATCH_IF (idle1 > 0 && idle5 > 0) BEGIN
        WRITE_LONG EXISTANCE4 idle4
        WRITE_LONG EXISTANCE5 idle5
      END
    END
  BUT_ONLY IF_EXISTS
END

// BGEE: correct existence slots offset references
ACTION_IF (GAME_IS ~bgee bg2ee eet~) BEGIN
  OUTER_SET EXISTANCE1 = 0x1b8
  OUTER_SET EXISTANCE2 = 0x1bc
  OUTER_SET EXISTANCE3 = 0x1c0
  OUTER_SET EXISTANCE4 = 0x1c4
  OUTER_SET EXISTANCE5 = 0x1c8
END

// correct BG1 rare selection slots offset references
ACTION_IF (GAME_IS ~bg2ee eet~) BEGIN
  OUTER_SET NSELECT_RARE1 = 0x1d0
  OUTER_SET NSELECT_RARE2 = 0x1d4
  OUTER_SET NSELECT_RARE3 = 0x1d8
  OUTER_SET NSELECT_RARE4 = 0x1dc
END
ELSE BEGIN
  OUTER_SET NSELECT_RARE1 = 0x130
  OUTER_SET NSELECT_RARE2 = 0x134
  OUTER_SET NSELECT_RARE3 = 0x138
  OUTER_SET NSELECT_RARE4 = 0x13c
END

