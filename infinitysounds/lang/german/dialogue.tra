@10087 = ~Nein!~ [ellesi36]
@10088 = ~Ich muss fliehen!~ [garren20]
@10089 = ~In den Schatten! Ich muss mich verstecken!~ [jariel20]
@10090 = ~Oje! Lauft! Lauft!~ [kalah26]
@10091 = ~Ich muss zurückfallen!~ [kaylxx20]
@10092 = ~Rückzug!~ [kpdomo25]
@10093 = ~Ich … ich muss fliehen!~ [lavok31]
@10094 = ~Ich … ich muss fliehen! Lasst mich in Ruhe!~ [mariax20]
@10095 = ~Sturmfürst, habt Erbarmen!~ [nallab20]
@10096 = ~Helm, meine Kräfte verlassen mich!~ [oisigx20]
@10097 = ~Heft mir! Heft mir, bitte!~ [phaere23]
@10098 = ~Nein! Neeein! Ich muss f…fort!~ [rielev20]
@10099 = ~Morgenfürst, beschütze mich!~ [thaddi20]
@10100 = ~Ich werde wiederkehren! Merkt Euch meine Worte~ [trgeni25]
