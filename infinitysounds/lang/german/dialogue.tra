@187 = ~Nein!~ [ellesi36]
@188 = ~Ich muss fliehen!~ [garren20]
@189 = ~In den Schatten! Ich muss mich verstecken!~ [jariel20]
@190 = ~Oje! Lauft! Lauft!~ [kalah26]
@191 = ~Ich muss zurückfallen!~ [kaylxx20]
@192 = ~Rückzug!~ [kpdomo25]
@193 = ~Ich … ich muss fliehen!~ [lavok31]
@194 = ~Ich … ich muss fliehen! Lasst mich in Ruhe!~ [mariax20]
@195 = ~Sturmfürst, habt Erbarmen!~ [nallab20]
@196 = ~Helm, meine Kräfte verlassen mich!~ [oisigx20]
@197 = ~Heft mir! Heft mir, bitte!~ [phaere23]
@198 = ~Nein! Neeein! Ich muss f…fort!~ [rielev20]
@199 = ~Morgenfürst, beschütze mich!~ [thaddi20]
@200 = ~Ich werde wiederkehren! Merkt Euch meine Worte~ [trgeni25]
