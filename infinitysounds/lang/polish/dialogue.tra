@10000 = ~Przysięgam, oderwę ci głowę od reszty ciała!~ [celoga28]
@10001 = ~Bogowie poprowadzą mój miecz, by wbił się w twoje czarne serce!~ [celoga29]
@10002 = ~Witam was i pozdrawiam, przyjaciele!~ [celoga21]
@10003 = ~Niechaj bogowie wam sprzyjają.~ [celoga22]
@10004 = ~Miło mi poznać.~ [celoga23]
@10005 = ~Podli i nikczemni jesteście!~ [celoga24]
@10006 = ~Nie! Nie mogę cię pokonać!~ [cor22]
@10007 = ~Z przyjemnością wyrwę ci serce!~ [cor18]
@10008 = ~Skrzyżuj ze mną ostrze, a zginiesz!~ [cor19]
@10009 = ~Chodź, przyjacielu, porozmawiajmy zatem.~ [cor11]
@10010 = ~Jak sobie życzysz.~ [cor12]
@10011 = ~Byle szybko.~ [cor13]
@10012 = ~Kończy się moja tolerancja!~ [cor14]
@10013 = ~Demon! Wściekły demon!~ [firkra29]
@10014 = ~Dopilnuję, żeby cię powiesili!~ [firkra25]
@10015 = ~Oddychaj, póki możesz. Zapłacisz za to!~ [firkra26]
@10016 = ~Będę cię tolerować tak długo, jak długo będziesz się właściwie zachowywać.~ [firkra18]
@10017 = ~~ [firkra19]
@10018 = ~Chyba wiesz, co to takiego kąpiel?~ [firkra20]
@10019 = ~Odejdź albo wezwę straże!~ [firkra21]
@10020 = ~To jeszcze nie koniec!~ [gaal37]
@10021 = ~Mam niewiele cierpliwości dla takich jak ty!~ [gaal29]
@10022 = ~Mam już tego dość!~ [gaal33]
@10023 = ~Wydajesz się być godną osobą.~ [gaal26]
@10024 = ~Mam ważniejsze rzeczy do zrobienia.~ [gaal27]
@10025 = ~~ [gaal28]
@10026 = ~Strzeż się, wiem kim jesteś.~ [gaal34]
@10027 = ~Uważaj, bo zawiśniesz na własnym języku!~ [bregax16]
@10028 = ~~ [bregax18]
@10029 = ~Witaj!~ [bregax10]
@10030 = ~Wszystko w porządku?~ [bregax11]
@10031 = ~Nie mam czasu na rozmowy z takimi jak ty.~ [bregax12]
@10032 = ~Nie pozwolę, żeby taka hołota jak wy coś mi zrobiła!~ [isaeax20]
@10033 = ~Usunę twój plugawy ród z powierzchni Krain!~ [isaeax16]
@10034 = ~Nie zasługujesz na nic oprócz śmierci.~ [isaeax17]
@10035 = ~Witajcie!~ [isaeax09]
@10036 = ~Czy to odpowiednie miejsce na rozmowę?~ [isaeax10]
